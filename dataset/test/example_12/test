v0x6000026386c0_0 .var "input_val", 7 0;
TD_tb_permute_8bit.expected_permutation ;
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 7, 4;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 1, 2;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 4, 4;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 0, 2;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 6, 4;
    %ix/load 4, 5, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 6, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
    %load/vec4 v0x6000026386c0_0;
    %parti/s 1, 5, 4;
    %ix/load 4, 7, 0;
    %flag_set/imm 4, 0;
    %ret/vec4 0, 4, 1; Assign to expected_permutation (store_vec4_to_lval)
