T_3 ;
    %wait E_0x600000014680;
    %load/vec4 v0x60000271c480_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_3.0, 6;
    %dup/vec4;
