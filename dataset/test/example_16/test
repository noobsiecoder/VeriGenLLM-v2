S_0x141e0a360 .scope module, "tb_fsm_detect_101" "tb_fsm_detect_101" 2 3;
 .timescale -9 -12;
v0x60000271c750_0 .var "clk", 0 0;
v0x60000271c7e0_0 .net "detected", 0 0, v0x60000271c510_0;  1 drivers
v0x60000271c870_0 .var "in", 0 0;
v0x60000271c900_0 .var/i "pass_count", 31 0;
v0x60000271c990_0 .var "rst", 0 0;
v0x60000271ca20_0 .var/i "test_count", 31 0;
v0x60000271cab0_0 .var "test_passed", 0 0;
S_0x141e098a0 .scope task, "send_bit_and_check" "send_bit_and_check" 2 31, 2 31 0, S_0x141e0a360;
 .timescale -9 -12;
v0x60000271c000_0 .var "bit_value", 0 0;
v0x60000271c090_0 .var "description", 127 0;
v0x60000271c120_0 .var "expected_detected", 0 0;
E_0x600000014540 .event posedge, v0x60000271c3f0_0;
E_0x600000014580 .event negedge, v0x60000271c3f0_0;
TD_tb_fsm_detect_101.send_bit_and_check ;
    %wait E_0x600000014580;
    %load/vec4 v0x60000271c000_0;
    %store/vec4 v0x60000271c870_0, 0, 1;
    %wait E_0x600000014540;
    %delay 1000, 0;
    %load/vec4 v0x60000271ca20_0;
    %addi 1, 0, 32;
    %store/vec4 v0x60000271ca20_0, 0, 32;
    %vpi_call 2 42 "$display", "Test %0d: %0s", v0x60000271ca20_0, v0x60000271c090_0 {0 0 0};
    %vpi_call 2 43 "$display", "  Input bit: %b, Expected detected: %b, Got: %b", v0x60000271c000_0, v0x60000271c120_0, v0x60000271c7e0_0 {0 0 0};
    %load/vec4 v0x60000271c7e0_0;
    %load/vec4 v0x60000271c120_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 6;
    %vpi_call 2 47 "$display", "  PASS" {0 0 0};
    %load/vec4 v0x60000271c900_0;
    %addi 1, 0, 32;
    %store/vec4 v0x60000271c900_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 50 "$display", "  FAIL" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60000271cab0_0, 0, 1;
T_0.1 ;
    %end;
S_0x141e0b0a0 .scope task, "send_sequence" "send_sequence" 2 57, 2 57 0, S_0x141e0a360;
 .timescale -9 -12;
v0x60000271c1b0_0 .var "description", 127 0;
v0x60000271c240_0 .var/i "j", 31 0;
v0x60000271c2d0_0 .var "length", 4 0;
v0x60000271c360_0 .var "sequence", 31 0;
TD_tb_fsm_detect_101.send_sequence ;
    %vpi_call 2 63 "$display", "\012Sending sequence: %0s", v0x60000271c1b0_0 {0 0 0};
    %load/vec4 v0x60000271c2d0_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %store/vec4 v0x60000271c240_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x60000271c240_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_1.3, 5;
    %wait E_0x600000014580;
    %load/vec4 v0x60000271c360_0;
    %load/vec4 v0x60000271c240_0;
    %part/s 1;
    %store/vec4 v0x60000271c870_0, 0, 1;
    %wait E_0x600000014540;
    %delay 1000, 0;
    %load/vec4 v0x60000271c2d0_0;
    %pad/u 32;
    %load/vec4 v0x60000271c240_0;
    %sub;
    %vpi_call 2 69 "$display", "  Bit %0d: in=%b, detected=%b", S<0,vec4,u32>, &PV<v0x60000271c360_0, v0x60000271c240_0, 1>, v0x60000271c7e0_0 {1 0 0};
    %load/vec4 v0x60000271c240_0;
    %subi 1, 0, 32;
    %store/vec4 v0x60000271c240_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
    %end;
